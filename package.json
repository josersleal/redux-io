{
  "name": "@shoutem/redux-io",
  "version": "3.1.0",
  "description": "Action creators, reducers and middleware for simple handling of normalized json api data in state",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src test",
    "test": "mocha --require babelTestSetup --reporter spec --recursive test",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report text --report html node_modules/mocha/bin/_mocha -- -R spec --recursive test",
    "build": "babel src --out-dir lib && npm run move",
    "move": "copyfiles package.json .npmignore lib",
    "pack-lib": "npm pack lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git@bitbucket.org:fiveminutes/redux-api-state.git"
  },
  "keywords": [
    "redux",
    "json-api",
    "normalize",
    "denormalize",
    "simple",
    "crud"
  ],
  "author": "Shoutem",
  "license": "MIT",
  "bugs": {
    "url": "https://bitbucket.org/fiveminutes/redux-api-state/issues"
  },
  "homepage": "https://bitbucket.org/fiveminutes/redux-api-state#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-shallow-deep-equal": "^1.4.0",
    "copyfiles": "^1.2.0",
    "cross-fetch": "^1.1.1",
    "deep-freeze": "0.0.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "estraverse-fb": "^1.3.1",
    "isparta": "^4.0.0",
    "istanbul": "0.4.4",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "redux-mock-store": "^1.1.0",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "@shoutem/json-api-denormalizer": "0.4.0-beta.2",
    "lodash": "^4.17.15",
    "memoize-one": "^3.1.1",
    "redux": "^3.3.1",
    "redux-api-middleware": "2.0.1",
    "redux-batched-actions": "^0.1.2",
    "redux-thunk": "^2.0.0",
    "urijs": "^1.18.10",
    "uuid": "^7.0.2",
    "vedrani-json-api-normalizr": "^3.2.4",
    "z-schema": "^3.17.0"
  }
}
